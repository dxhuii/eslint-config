{
  "name": "@dxhuii/eslint-config",
  "type": "module",
  "version": "0.0.1-beta.19",
  "packageManager": "pnpm@8.8.0",
  "description": "Dxhuii's ESLint config",
  "author": "Dxhuii <dxhuii@hotmail.com> (https://github.com/dxhuii)",
  "license": "MIT",
  "homepage": "https://github.com/dxhuii/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm --clean",
    "lint": "pnpm run stub && eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.7.4",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.21",
    "@antfu/ni": "^0.21.8",
    "@dxhuii/eslint-config": "workspace:*",
    "@types/eslint": "^8.44.3",
    "@types/node": "^20.8.3",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
